{
  "name": "gingko-client",
  "productName": "Gingko",
  "version": "0.1.7",
  "description": "Gingko client rewritten in Elm, packaged with Electron.",
  "repository": {
    "type": "git",
    "url": "git://github.com/AdrianoFerrari/gingko-client.git"
  },
  "author": {
    "name": "Adriano Ferrari",
    "email": "adriano.ferrari@gmail.com"
  },
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/AdrianoFerrari/gingko-client/issues"
  },
  "homepage": "https://github.com/AdrianoFerrari/gingko-client",
  "scripts": {
    "start": "webpack --watch",
    "electron": "electron ./app",
    "pack": "electron-builder --dir",
    "dist": "electron-builder",
    "dist:win": "electron-builder -w",
    "dist:mac": "electron-builder -m"
  },
  "build": {
    "appId": "com.gingkoapp.desktop",
    "linux": {
      "target": [
        "AppImage"
      ]
    },
    "win": {
      "target": "squirrel",
      "icon": "build/leaf256.ico"
    },
    "mac": {
      "target": "tar.gz"
    }
  },
  "dependencies": {
    "dblsqd-electron": "^0.3.2",
    "electron-machine-id": "^1.0.9",
    "electron-squirrel-startup": "^1.0.0",
    "jquery": "^3.1.1",
    "lodash": "^4.17.4",
    "mz": "^2.6.0",
    "pouchdb-adapter-memory": "^6.3.4",
    "pouchdb-browser": "^6.3.4",
    "pouchdb-replication-stream": "^1.2.9",
    "react": "^15.5.4",
    "react-commits-graph": "git+https://github.com/AdrianoFerrari/react-commits-graph.git",
    "react-dom": "^15.5.4",
    "sha1": "^1.1.1",
    "socket.io-client": "^2.0.1",
    "textarea-autosize": "^0.4.2",
    "underscore": "^1.8.3"
  },
  "devDependencies": {
    "chokidar-cli": "^1.2.0",
    "copy-webpack-plugin": "^4.0.1",
    "electron": "^1.7.6",
    "electron-builder": "^19.27.0",
    "electron-builder-squirrel-windows": "^19.35.1",
    "electron-debug": "^1.4.0",
    "electron-packager": "^8.7.2",
    "electron-rebuild": "^1.6.0",
    "elm-webpack-loader": "^4.3.0",
    "file-loader": "^0.11.2",
    "html-webpack-plugin": "^2.29.0",
    "imports-loader": "^0.7.1",
    "webpack": "^3.5.5",
    "webpack-dev-server": "^2.7.1"
  }
}
